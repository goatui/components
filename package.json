{
  "name": "@p4rm/ui",
  "version": "1.2.2",
  "description": "P4RM UI",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "unpkg": "dist/p4rm-ui/p4rm-ui.js",
  "module": "dist/custom-elements/index.js",
  "types": "dist/custom-elements/index.d.ts",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "concurrently --kill-others \"npm run start:watch\" \"npm run start:docs-server\"",
    "start:watch": "stencil build --dev --watch --serve --cors",
    "start:docs-server": "cd docs && serve.cmd",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "release": "npm version patch",
    "release-to-docs": "gulp releaseToDocs"
  },
  "dependencies": {
    "@stencil/core": "^2.8.0",
    "@stencil/sass": "^1.4.1",
    "bootstrap-icons": "^1.6.1",
    "stencil-inline-svg": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.9.6",
    "concurrently": "^6.2.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "sass": "^1.42.1",
    "workbox-build": "^4.3.1"
  },
  "engines": {
    "node": ">=14.x"
  },
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs"
}
